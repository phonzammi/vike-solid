{
  "name": "vike-solid-query",
  "version": "0.1.0",
  "type": "module",
  "main": "./dist/src/index/server.js",
  "module": "./dist/src/index/server.js",
  "types": "./dist/src/index.d.ts",
  "browser": {
    "./dist/src/index/server.js": "./dist/src/index.js"
  },
  "exports": {
    ".": {
      "worker": {
        "import": {
          "types": "./dist/src/index.d.ts",
          "default": "./dist/src/index/server.js"
        }
      },
      "browser": {
        "import": {
          "types": "./dist/src/index.d.ts",
          "default": "./dist/src/index.js"
        }
      },
      "deno": {
        "import": {
          "types": "./dist/src/index.d.ts",
          "default": "./dist/src/index/server.js"
        }
      },
      "node": {
        "import": {
          "types": "./dist/src/index.d.ts",
          "default": "./dist/src/index/server.js"
        }
      },
      "import": {
        "types": "./dist/src/index.d.ts",
        "default": "./dist/src/index.js"
      }
    },
    "./config": {
        "types": "./dist/integration/_config.d.ts",
        "default": "./dist/integration/+config.js"
    },
    "./__internal/integration/Wrapper": {
      "solid": "./dist/integration/Wrapper.js",
      "import": {
        "types": "./dist/integration/Wrapper.d.ts",
        "default": "./dist/integration/Wrapper.js"
      }
    }
  },
  "scripts": {
    "dev": "tsc --watch",
    "dev:typecheck": "tsc --noEmit --watch",
    "build": "rimraf dist/ && tsup",
    "release": "LANG=en_US release-me patch",
    "release:minor": "LANG=en_US release-me minor",
    "release:commit": "LANG=en_US release-me commit"
  },
  "peerDependencies": {
    "@tanstack/solid-query": ">=5.0.0",
    "solid-js": "^1.8.7",
    "vike-solid": "workspace:^"
  },
  "devDependencies": {
    "@brillout/release-me": "^0.4.2",
    "@tanstack/solid-query": "^5.59.13",
    "@types/node": "^22.5.5",
    "rimraf": "^6.0.1",
    "solid-js": "^1.9.2",
    "tsup": "^8.3.0",
    "tsup-preset-solid": "^2.2.0",
    "typescript": "^5.6.3",
    "vike": "^0.4.199",
    "vike-solid": "workspace:*"
  },
  "typesVersions": {
    "*": {
      "config": [
        "./dist/integration/+config.d.ts"
      ],
      "__internal/integration/Wrapper": [
        "./dist/integration/Wrapper.d.ts"
      ]
    }
  },
  "files": [
    "dist/"
  ],
  "repository": "github:vikejs/vike-solid",
  "license": "MIT"
}
